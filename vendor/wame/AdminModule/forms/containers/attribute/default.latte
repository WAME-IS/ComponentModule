{input $container['attribute']['add']}

{foreach $container['attribute']->getComponents() as $attr}
    {if $attr instanceof \Nette\Forms\Container && isset($attr['name'])}
    <div class="form-group">
        {label $attr['name']}
        <input n:name="$attr['name']">
    </div>
        
    <div class="form-group">
        {label $attr['value']}
        <input n:name="$attr['value']">
    </div>
    {/if}
{/foreach}
    